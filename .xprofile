# networkmanager
nm-applet &

# auto-lock screen
xautolock -time 5 -locker "~/.dotfiles/i3lock/lock.sh" &

# tray-icon for updates
pamac-tray &

# bspwm fix for jdownloader
wmname LG3D

# email client
thunderbird &

# chat client
rambox &

# status bar
polybar top-work &

# usenet client daemon
nzbget -D
